<?xml version="1.0" encoding="UTF-8"?>
<!--
  modules.xml 文件
  该文件用于存储项目中的模块信息，通常由 IDE（如 IntelliJ IDEA）自动生成。
  它包含了项目中所有模块的配置，以便于 IDE 知道如何加载和管理这些模块。
-->

<project version="4">
  <!-- ProjectModuleManager 组件，管理项目中的所有模块 -->
  <component name="ProjectModuleManager">
    <!-- modules 元素包含项目中的所有模块信息 -->
    <modules>
      <!-- module 元素代表一个模块 -->
      <!-- 'fileurl' 属性是模块文件的 URL 路径，'filepath' 属性是模块文件的相对路径 -->
      <module fileurl="file://$PROJECT_DIR$/.idea/video-player-app.iml" 
              filepath="$PROJECT_DIR$/.idea/video-player-app.iml" />
    </modules>
  </component>
</project>
